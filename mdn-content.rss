        &lt;button id=&quot;dialogCloseButton&quot;&gt;close dialog&lt;/button&gt;
      &lt;/dialog&gt;
      &lt;button id=&quot;dialogOpenButton&quot;&gt;open dialog&lt;/button&gt;
