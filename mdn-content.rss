    <span class="pl-kos">&lt;</span><span class="pl-ent">div</span><span class="pl-kos">&gt;</span>5000sandbox.html<span class="pl-kos">&lt;/</span><span class="pl-ent">div</span><span class="pl-kos">&gt;</span>
    <span class="pl-kos">&lt;</span><span class="pl-ent">iframe</span> <span class="pl-c1">src</span>="<span class="pl-s">http://localhost:5000/sandbox</span>" <span class="pl-c1">sandbox</span>="<span class="pl-s">allow-scripts</span>"<span class="pl-kos">&gt;</span><span class="pl-kos">&lt;/</span><span class="pl-ent">iframe</span><span class="pl-kos">&gt;</span>
